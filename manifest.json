{
    "manifest_version": "1",
    "api_version": "1",
    "name": "Hash",
    "description": "Generate hashes from a string, using common algoriths like md5, sha1, sha256",
    "developer_name": "Bruno Paz",
    "icon": "images/icon.png",
    "options": {
        "query_debounce": 0.05
    },
    "preferences": [
        {
            "id": "hash_kw",
            "type": "keyword",
            "name": "Hash",
            "default_value": "hash"
        },
        {
